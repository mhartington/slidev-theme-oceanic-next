{
  "name": "slidev-theme-oceanic-next",
  "version": "0.0.22",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=14.0.0",
    "slidev": ">=0.49.0"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "NODE_ENV=development slidev example.md",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^0.50.0-beta.5",
    "codemirror-theme-vars": "^0.1.1",
    "prism-theme-vars": "^0.2.4",
    "theme-vitesse": "^0.6.2"
  },
  "devDependencies": {
    "@slidev/cli": "^0.50.0-beta.5",
    "@types/node": "^18.13.0"
  },
  "slidev": {
    "colorSchema": "dark",
    "highlighter": "shiki",
    "defaults": {
      "fonts": {
        "mono": "Fira Code",
        "sans": "Avenir Next,Nunito Sans",
        "local": "Avenir Next"
      }
    }
  },
  "author": "Mike Hartington <mikehartington@gmail.com>"
}
